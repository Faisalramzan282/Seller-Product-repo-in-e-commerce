t totalPRICE = this.carts.reduce((total, carts) => total + carts.price, 0);\r\n         if(totalPRICE !== 0)\r\n         {\r\n          return totalPRICE;\r\n         }\r\n         else{\r\n          return CartShow;\r\n         }\r\n         \r\n        }\r\n       \r\n      \r\n    }, \r\n    data(){\r\n        return{ \r\n            count: 1,\r\n            \r\n        }\r\n    },\r\n    methods:{\r\n        ...mapActions({cartsCoun: 'cartsCount'}),\r\n        removeCart(idx)\r\n        {\r\n            this.carts.splice(idx, 1);\r\n        },\r\n        plusCount(task){\r\n          const updateTask = {\r\n            ...task,\r\n            count: this.count\r\n          }\r\n          this.cartsCoun(updateTask);\r\n\r\n        },\r\n        minusCount(idx)\r\n        {\r\n          this.carts.splice(idx, 1);\r\n        }, \r\n        \r\n        \r\n    }, \r\n   \r\n\r\n}\r\n\r\n</script>\r\n<style>\r\n\r\n  @media screen and (max-width: 640px) {\r\n  .specific-width{\r\n    width: 98%;\r\n  \r\n  }\r\n}\r\n\r\n\r\n</style>"],"mappings":"AA8DA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,UAAS,MAAO,kBAAkB;AACzC,eAAc;EACTC,UAAU,EAAC;IACVD;EACD,CAAC;EACFE,IAAI,EAAE,WAAW;EACjBC,QAAQ,EAAC;IACL,GAAGL,UAAU,CAAC;MAACM,KAAK,EAAE;IAAU,CAAC,CAAC;IAElCC,UAAUA,CAAA,EAAE;MACV,IAAIC,QAAO,GAAI,SAAS;MACzB,IAAIC,UAAS,GAAI,IAAI,CAACH,KAAK,CAACI,MAAM,CAAC,CAACC,KAAK,EAAEL,KAAK,KAAKK,KAAI,GAAIL,KAAK,CAACM,KAAK,EAAE,CAAC,CAAC;MAC5E,IAAGH,UAAS,KAAM,CAAC,EACnB;QACC,OAAOA,UAAU;MAClB,OACI;QACH,OAAOD,QAAQ;MAChB;IAED;EAGJ,CAAC;EACDK,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAE;IAEX;EACJ,CAAC;EACDC,OAAO,EAAC;IACJ,GAAGd,UAAU,CAAC;MAACe,SAAS,EAAE;IAAY,CAAC,CAAC;IACxCC,UAAUA,CAACC,GAAG,EACd;MACI,IAAI,CAACZ,KAAK,CAACa,MAAM,CAACD,GAAG,EAAE,CAAC,CAAC;IAC7B,CAAC;IACDE,SAASA,CAACC,IAAI,EAAC;MACb,MAAMC,UAAS,GAAI;QACjB,GAAGD,IAAI;QACPP,KAAK,EAAE,IAAI,CAACA;MACd;MACA,IAAI,CAACE,SAAS,CAACM,UAAU,CAAC;IAE5B,CAAC;IACDC,UAAUA,CAACL,GAAG,EACd;MACE,IAAI,CAACZ,KAAK,CAACa,MAAM,CAACD,GAAG,EAAE,CAAC,CAAC;IAC3B;E