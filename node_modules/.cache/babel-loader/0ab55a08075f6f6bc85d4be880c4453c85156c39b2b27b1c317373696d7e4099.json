{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nexport default {\n  name: \"ShowCarts\",\n  computed: {\n    ...mapGetters({\n      carts: \"getCarts\"\n    })\n  },\n  data() {\n    return {\n      count: 1\n    };\n  },\n  methods: {\n    ...mapActions({\n      cartsCoun: 'cartsCount'\n    }),\n    removeCart(idx) {\n      this.carts.splice(idx, 1);\n    },\n    plusCount(task, idx) {\n      console.log(task);\n      this.cartsCoun(task);\n      console.log(\"addcarts count is : \", idx);\n    },\n    minusCount(idx) {\n      this.carts.splice(idx, 1);\n    },\n    totalSum(idx) {\n      let sum = 0;\n      for (let i = 0; i < this.carts.length; i++) {\n        sum = sum + this.carts[idx].price;\n        idx++;\n      }\n      console.log(\"sum of carts for checking is ++>\", sum);\n      return sum;\n    }\n  },\n  created() {}\n};","map":{"version":3,"names":["mapGetters","mapActions","name","computed","carts","data","count","methods","cartsCoun","removeCart","idx","splice","plusCount","task","console","log","minusCount","totalSum","sum","i","length","price","created"],"sources":["D:\\Data\\Web Development\\TECNIQE\\15_CRUD_Cart_IN VUE JS\\PRODUCT_CRUD_OPERATION\\src\\components\\CartsDisplay.vue"],"sourcesContent":["<template>\r\n    <div>\r\n       <div class=\"flex justify-end m-2\">\r\n        <h2 class=\"m-4\">Total Favourite Carts: {{ carts.length }}</h2>\r\n        <p class=\"border border-2 inline-block max-w-[100%] cursor-pointer px-4 bg-stone-200 py-4 rounded-lg\" @click=\"carts.length=0\">Remove All</p>\r\n\r\n       </div>\r\n    <ul>\r\n        <li v-for=\"(task,idx) in carts\" :key =idx>\r\n            <div\r\n            class=\"w-1/2 border border-2 border-solid divide-y divide-slate-200 mt-8 mb-8 mx-auto specific-width-cont\"\r\n          >\r\n            <p class=\"text-start p-4\"><b>Title:</b> {{ task.title }}</p>\r\n            <p class=\"text-start p-4\"><b>Description:</b> {{ task.desc }}</p>\r\n            <p  class=\"text-start p-4\"> <b>Price:</b>{{ task.price }}</p>\r\n           \r\n            <button \r\n              @click=\"removeCart(idx)\"\r\n              class=\" w-1/2 flex mx-auto m-2 items-center justify-center bg-blue-500 text-white px-4 py-2 rounded\"\r\n            >\r\n              Remove Cart\r\n            </button>\r\n            <div class=\"flex flex-row gap-4 justify-center\">\r\n                <img\r\n               \r\n                @click=\"plusCount(task,idx)\"\r\n                class=\"w-1/12 cursor-pointer\"\r\n                src=\"../assets/icons/plus.png\"\r\n                alt=\"plus\"\r\n              />\r\n               <!-- <h2 class=\"text-3xl\">{{ count }}</h2> -->\r\n              <img\r\n              @click=\"minusCount(idx, task)\"\r\n                class=\"w-1/12 cursor-pointer\"\r\n                src=\"../assets/icons/minus-sign.png\"\r\n                alt=\"minus\"\r\n              />\r\n            </div>\r\n            </div>\r\n            \r\n        </li>\r\n    </ul>\r\n        <button @dblclick=\"totalSum\">sum : {{ sum }}</button>\r\n    </div>\r\n    \r\n      \r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from 'vuex';\r\nexport default{\r\n    name: \"ShowCarts\",\r\n    computed:{\r\n        ...mapGetters({carts: \"getCarts\"}),\r\n        \r\n      \r\n    }, \r\n    data(){\r\n        return{ \r\n            count: 1,\r\n            \r\n        }\r\n    },\r\n    methods:{\r\n        ...mapActions({cartsCoun: 'cartsCount'}),\r\n        removeCart(idx)\r\n        {\r\n            this.carts.splice(idx, 1);\r\n        },\r\n        plusCount(task,idx){\r\n            console.log(task);\r\n            this.cartsCoun(task);\r\n            console.log(\"addcarts count is : \", idx)\r\n        },\r\n        minusCount(idx)\r\n        {\r\n          this.carts.splice(idx, 1);\r\n        }, \r\n        totalSum(idx){\r\n          let sum =0;\r\n          for(let i =0 ; i<this.carts.length; i++)\r\n          {\r\n                 sum = sum + this.carts[idx].price;\r\n                 idx++;\r\n          }\r\n          console.log(\"sum of carts for checking is ++>\", sum);\r\n          return sum;\r\n          \r\n        }\r\n        \r\n    }, \r\n    created(){\r\n        \r\n    }\r\n\r\n}\r\n\r\n</script>"],"mappings":"AAiDA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,eAAc;EACVC,IAAI,EAAE,WAAW;EACjBC,QAAQ,EAAC;IACL,GAAGH,UAAU,CAAC;MAACI,KAAK,EAAE;IAAU,CAAC;EAGrC,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,KAAK,EAAE;IAEX;EACJ,CAAC;EACDC,OAAO,EAAC;IACJ,GAAGN,UAAU,CAAC;MAACO,SAAS,EAAE;IAAY,CAAC,CAAC;IACxCC,UAAUA,CAACC,GAAG,EACd;MACI,IAAI,CAACN,KAAK,CAACO,MAAM,CAACD,GAAG,EAAE,CAAC,CAAC;IAC7B,CAAC;IACDE,SAASA,CAACC,IAAI,EAACH,GAAG,EAAC;MACfI,OAAO,CAACC,GAAG,CAACF,IAAI,CAAC;MACjB,IAAI,CAACL,SAAS,CAACK,IAAI,CAAC;MACpBC,OAAO,CAACC,GAAG,CAAC,sBAAsB,EAAEL,GAAG;IAC3C,CAAC;IACDM,UAAUA,CAACN,GAAG,EACd;MACE,IAAI,CAACN,KAAK,CAACO,MAAM,CAACD,GAAG,EAAE,CAAC,CAAC;IAC3B,CAAC;IACDO,QAAQA,CAACP,GAAG,EAAC;MACX,IAAIQ,GAAE,GAAG,CAAC;MACV,KAAI,IAAIC,CAAA,GAAG,GAAIA,CAAC,GAAC,IAAI,CAACf,KAAK,CAACgB,MAAM,EAAED,CAAC,EAAE,EACvC;QACOD,GAAE,GAAIA,GAAE,GAAI,IAAI,CAACd,KAAK,CAACM,GAAG,CAAC,CAACW,KAAK;QACjCX,GAAG,EAAE;MACZ;MACAI,OAAO,CAACC,GAAG,CAAC,kCAAkC,EAAEG,GAAG,CAAC;MACpD,OAAOA,GAAG;IAEZ;EAEJ,CAAC;EACDI,OAAOA,CAAA,EAAE,CAET;AAEJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}